<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.RyMoxieMapper">
    
    <resultMap type="RyMoxie" id="RyMoxieResult">
        <result property="id"    column="id"    />
        <result property="userId"    column="user_id"    />
        <result property="moxieUrl"    column="moxie_url"    />
        <result property="status"    column="status"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectRyMoxieVo">
        select id, user_id, moxie_url, status, create_time, update_time from ry_moxie
    </sql>

    <select id="selectRyMoxieList" parameterType="RyMoxie" resultMap="RyMoxieResult">
        <include refid="selectRyMoxieVo"/>
        <where>  
        </where>
    </select>
    
    <select id="selectRyMoxieById" parameterType="Long" resultMap="RyMoxieResult">
        <include refid="selectRyMoxieVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertRyMoxie" parameterType="RyMoxie" useGeneratedKeys="true" keyProperty="id">
        insert into ry_moxie
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null ">user_id,</if>
            <if test="moxieUrl != null  and moxieUrl != ''">moxie_url,</if>
            <if test="status != null ">status,</if>
            <if test="createTime != null ">create_time,</if>
            <if test="updateTime != null ">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null ">#{userId},</if>
            <if test="moxieUrl != null  and moxieUrl != ''">#{moxieUrl},</if>
            <if test="status != null ">#{status},</if>
            <if test="createTime != null ">#{createTime},</if>
            <if test="updateTime != null ">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateRyMoxie" parameterType="RyMoxie">
        update ry_moxie
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null ">user_id = #{userId},</if>
            <if test="moxieUrl != null  and moxieUrl != ''">moxie_url = #{moxieUrl},</if>
            <if test="status != null ">status = #{status},</if>
            <if test="createTime != null ">create_time = #{createTime},</if>
            <if test="updateTime != null ">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteRyMoxieById" parameterType="Long">
        delete from ry_moxie where id = #{id}
    </delete>

    <delete id="deleteRyMoxieByIds" parameterType="String">
        delete from ry_moxie where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>